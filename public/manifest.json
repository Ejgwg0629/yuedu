{
  "name": "My First Extension",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/static/js/runtime-main.js",
        "/static/js/2.chunk.js",
        "/static/js/main.chunk.js"
      ],
      "css": [
        "App.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "management",
    "contextMenus",
    "https://*/*"
  ]
}